{
  "yaml.schemas": {
    "https://moonrepo.dev/schemas/tasks.json": [
      ".moon/tasks.yml",
      ".moon/tasks/**/*.yml"
    ],
    "https://moonrepo.dev/schemas/project.json": [
      "**/moon.yml"
    ],
    "https://moonrepo.dev/schemas/template.json": [
      "**/template.yml"
    ],
    "https://moonrepo.dev/schemas/workspace.json": [
      "**/.moon/workspace.yml"
    ],
    "https://moonrepo.dev/schemas/toolchain.json": [
      "**/.moon/toolchain.yml"
    ]
  },
  "cSpell.words": [
    "moonrepo",
    "packemon",
    "proto",
    "svallory",
    "tagit",
    "tokilabs",
    "vite",
    "vitest"
  ],
  "explorer.fileNesting.patterns": {
    ".config*": ".git*, prettier.*, babel.*, *config.toml, *config.yaml, *config.yml, *rc, *rc.json, *rc.js, *rc.mjs, *rc.cjs, *rc.ts, *rc.toml, *rc.yaml, *rc.yml, .eslint*, .prettier*, .*",
    "Configuration Files*": ".git*, prettier.*, babel.*, *config.toml, *config.yaml, *config.yml, *rc, *rc.json, *rc.js, *rc.mjs, *rc.cjs, *rc.ts, *rc.toml, *rc.yaml, *rc.yml, .eslint*, .prettier*, .*",
    "*.yml": "${basename}.*.yml"
  }
}